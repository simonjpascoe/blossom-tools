  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (ibkr-import)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_ibkr_import",
      "program": "${workspaceFolder}/ibkr-import/bin/Debug/netcoreapp3.1/ibkr-import.dll",
      "args": ["--parse", "blossom", "${workspaceFolder}/ibkr-import/bin/Debug/netcoreapp3.1/output.xml", "${workspaceFolder}/ibkr-import/bin/Debug/netcoreapp3.1/output.fledge"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
  ]
}