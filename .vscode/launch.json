  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (ibkr-import)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_ibkr_import",
      "program": "${workspaceFolder}/ibkr-import/bin/Debug/netcoreapp3.1/ibkr-import.dll",
      "args": ["--mode", "blossom",
               "--symbology", "g:\\personal-git-repos-local\\finance\\ibkr\\symbology.xml",
               "--output", "g:\\personal-git-repos-local\\finance\\ibkr\\omni.fledge",
               "--input", "g:\\personal-git-repos-local\\finance\\ibkr\\omni_2017.xml",
               "--input", "g:\\personal-git-repos-local\\finance\\ibkr\\omni_2018.xml",
               "--input", "g:\\personal-git-repos-local\\finance\\ibkr\\omni_2019.xml",
               "--input", "g:\\personal-git-repos-local\\finance\\ibkr\\omni_2020.xml",
               "--input", "g:\\personal-git-repos-local\\finance\\ibkr\\omni_2021.xml",
               ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
  ]
}